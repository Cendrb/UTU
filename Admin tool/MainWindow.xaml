<Window x:Class="Admin_tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Information Center Admin Tool" Height="122" Width="374" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="událostButton" Content="Událost" Margin="10,50,10,10" Click="událostButton_Click"/>
        <Button x:Name="testButton" Content="Test" Margin="10,50,10,10" Grid.Column="2" Click="testButton_Click"/>
        <Button x:Name="úkolButton" Content="Úkol" Margin="10,50,10,10" Grid.Column="1" Click="úkolButton_Click"/>
        <Label Grid.ColumnSpan="3" Content="Vyberte typ objektu pro vytvoření: " HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="uložitButton" Grid.ColumnSpan="2" Content="Uložit" Grid.Column="1" HorizontalAlignment="Left" Height="26" Margin="82,10,0,0" VerticalAlignment="Top" Width="152" Click="uložitButton_Click"/>
    </Grid>
</Window>
